          {
            "type": "definition",
            "term": "Personal Information",
            "definition": "Information that identifies, relates to, or can be used to identify you."
          },
          {
            "type": "definition",
            "term": "Service",
            "definition": "Our products, services, and website."
          }
        ]
      },
      {
        "title": "Information We Collect",
        "content": [
          {
            "type": "list",
            "items": [
              {
                "title": "Personal Information",
                "description": "name, email address, phone number, and other identifiers."
              },
              {
                "title": "Usage Data",
                "description": "browsing history, search queries, and interactions with the Service."
              },
              {
                "title": "Device Information",
                "description": "IP address, browser type, and device type."
              }
            ]
          }
        ]
      },
      {
